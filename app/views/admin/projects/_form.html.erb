<%= simple_form_for([@user, :admin, @project], :url => admin_projects_path) do |f| %>
<%= f.error_notification %>


  <div class="form-group">
    <div class="col-md-12">

    <%= f.input :title, class: "form-control" %>
  </div>
  </div>
  <div class="form-group">
    <div class="col-md-12">
    <%= f.input :content, class: "form-control", as: :ckeditor, input_html: { ckeditor: { toolbar: 'Full' } } %>
  </div>
</div>
<div class="form-group">
  <div class="col-md-12">
  <%= f.input :more, class: "form-control", as: :ckeditor, input_html: { ckeditor: { toolbar: 'Full' } } %>
</div>
</div>

<div class="form-group">
  <div class="col-md-12">
    <%= f.input :date, class: "form-control" %>
  </div>
</div>

<div class="form-group">
  <div class="col-md-12">
    <%= f.input :image, as: :file,  class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <div class="col-md-12">
    <%= f.submit :submit, class: "btn  btn-picasso" %>
  </div>
</div>
<% end %>
